{
  "version": 2,
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "src": "/livekit/(.*)",
      "dest": "https://reqviem.onrender.com/livekit/$1"
    },
    {
      "src": "/(?!.*\\.(?:js|mjs|css|png|jpg|jpeg|svg|ico|webp|webmanifest|json|map|woff2|woff|ttf)$)(.*)",
      "dest": "/index.html"
    }
  ]
}
